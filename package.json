{
  "name": "react-hooks-mock-code-challenge-plantshop",
  "version": "0.1.0",
  "homepage": "https://zippykitche.github.io/react-hooks-cc-plantshop",
  "private": true,
  "dependencies": {
    "json-server": "^1.0.0-beta.3",
    "node-fetch": "^2.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "uuid": "^11.0.2",
    "webpack-dev-server": "^5.1.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "npm install react-scripts && npx react-scripts build",
    "start": "react-scripts start",
    "server": "json-server --watch db.json -p 6001",
    "test": "jest --bail"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.4",
    "babel-jest": "^29.7.0",
    "es-abstract": "^1.23.3",
    "eslint-plugin-react": "^7.37.2",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "react-scripts": "^5.0.1",
    "string.prototype.repeat": "^1.0.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/test_suites"
    ]
  },
  "engines": {
    "node": "18.20.4"
  }
}
